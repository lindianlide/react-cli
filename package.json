{
    "description": "react-tsx-cli",
    "name": "react-tsx-cli",
    "version": "1.0.0",
    "scripts": {
        "start": "cross-env SELF_ENV=DEV webpack-dev-server --config=build/webpack.development.config.js",
        "dev": "nodemon --watch build --exec \"cross-env SELF_ENV=DEV webpack-dev-server --config=build/webpack.development.config.js\"",
        "build": "cross-env SELF_ENV=PRD webpack --config=build/webpack.production.config.js",
        "lint": "eslint --fix --ext .js ./src",
        "mock": "nodemon mock/service.js",
        "build-publish": "sh ./build.sh",
        "build-cp": "webpack --config=build/webpack.production.copy.config.js",
    },
    "devDependencies": {
        "@babel/core": "7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
        "@babel/plugin-proposal-optional-chaining": "^7.11.0",
        "@babel/plugin-syntax-dynamic-import": "7.0.0",
        "@babel/plugin-transform-flow-strip-types": "^7.10.4",
        "@babel/plugin-transform-regenerator": "7.0.0",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/preset-typescript": "^7.10.4",
        "@svgr/webpack": "4.3.2",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.1.0",
        "@typescript-eslint/parser": "^4.1.0",
        "art-template-loader": "1.4.3",
        "autoprefixer": "9.7.2",
        "babel-eslint": "10.0.2",
        "babel-loader": "8.0.6",
        "babel-preset-react-app": "^9.0.1",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "chalk": "2.4.1",
        "clean-webpack-plugin": "3.0.0",
        "core-js": "^3.6.5",
        "cross-env": "6.0.3",
        "css-loader": "2.1.1",
        "eslint": "^7.8.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-react-app": "^5.0.1",
        "eslint-loader": "2.2.1",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.1",
        "file-loader": "1.1.11",
        "filemanager-webpack-plugin": "^2.0.5",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "fs-extra": "^7.0.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "husky": "^3.1.0",
        "less": "3.10.3",
        "less-loader": "5.0.0",
        "lint-staged": "9.4.3",
        "mini-css-extract-plugin": "0.5.0",
        "nodemon": "^1.18.7",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "portfinder": "^1.0.28",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-px2rem": "0.3.0",
        "prettier": "^1.19.1",
        "react-app-polyfill": "^1.0.2",
        "react-dev-utils": "^9.0.3",
        "react-dev-utils-for-webpack4": "^5.0.0-webpack4",
        "resolve": "1.12.0",
        "resolve-url-loader": "3.1.0",
        "restify": "^8.5.1",
        "script-ext-html-webpack-plugin": "2.1.4",
        "style-loader": "1.0.0",
        "terser-webpack-plugin": "^1.2.3",
        "typescript": "^3.9.7",
        "url-loader": "2.1.0",
        "webpack": "4.39.1",
        "webpack-bundle-analyzer": "3.6.0",
        "webpack-cli": "3.0.8",
        "webpack-dev-server": "3.2.1",
        "webpack-manifest-plugin": "2.0.4",
        "webpack-merge": "4.2.2",
        "workbox-webpack-plugin": "4.3.1"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "@babel/runtime": "^7.12.1",
        "@babel/runtime-corejs3": "^7.11.2",
        "art-template": "4.13.2",
        "axios": "^0.19.2",
        "jsonp": "^0.2.1",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "swiper": "^5.2.1"
    },
    "lint-staged": {
        "*.js": [
            "node_modules/.bin/prettier --write",
            "node_modules/.bin/eslint --fix",
            "git add"
        ],
        "src/*.{css,scss,less,json,html,md,markdown}": [
            "node_modules/.bin/prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}